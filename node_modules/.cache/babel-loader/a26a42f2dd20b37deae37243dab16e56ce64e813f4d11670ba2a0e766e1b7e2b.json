{"ast":null,"code":"import { Line } from 'vue-chartjs';\nimport 'chart.js/auto';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Chart3\",\n  props: [\"rainArray\", \"rainFlagWithDate\", \"timeArray\", \"display\"],\n  components: {\n    Line\n  },\n  data() {\n    return {\n      rain: \"\",\n      title: \"\"\n    };\n  },\n  computed: {\n    chartData1() {\n      return {\n        labels: [this.timeArray],\n        backgroundColor: '#f87979',\n        datasets: [{\n          data: this.rainArray\n        }]\n      };\n    },\n    chartOptions1() {\n      return {\n        maintainAspectRatio: false,\n        layout: {\n          padding: {\n            top: 9,\n            bottom: 15\n          }\n        },\n        responsive: true,\n        scales: {\n          y: {\n            ticks: {\n              display: false\n            },\n            grid: {\n              drawTicks: false,\n              drawBorder: false\n            }\n          }\n        },\n        plugins: {\n          legend: {\n            display: false\n          },\n          tooltip: {\n            callbacks: {\n              label: function (context) {\n                return context.parsed.y + \"mm\";\n              }\n            }\n          }\n        }\n      };\n    },\n    chartData2() {\n      return {\n        labels: [this.timeArray],\n        backgroundColor: '#f87979',\n        datasets: [{\n          data: this.rainArray,\n          borderWidth: 0\n        }]\n      };\n    },\n    chartOptions2() {\n      return {\n        maintainAspectRatio: false,\n        layout: {\n          padding: {\n            bottom: 44\n          }\n        },\n        responsive: true,\n        scales: {\n          x: {\n            ticks: {\n              display: false\n            },\n            grid: {\n              drawTicks: false\n            }\n          },\n          y: {\n            afterFit: ctx => {\n              ctx.width = 35;\n            }\n          }\n        },\n        plugins: {\n          legend: {\n            display: false\n          }\n        },\n        elements: {\n          point: {\n            radius: 0\n          }\n        }\n      };\n    }\n  },\n  watch: {\n    rainArray: function (newData) {\n      this.chartData1.datasets[0].data = newData;\n      this.chartData2.datasets[0].data = newData;\n      this.title = \"Rain Volume(mm/3h)\";\n    },\n    timeArray: function (newData) {\n      this.chartData1.labels = newData;\n      this.chartData2.labels = newData;\n    },\n    rainFlagWithDate: function (newData) {\n      const rainFlag = newData;\n      let text = \"You should pack an umbrella on \";\n      let forIfFlag = false;\n      for (let key in rainFlag) {\n        if (rainFlag[key]) {\n          text = text.concat(key + \", \");\n          forIfFlag = true;\n        }\n      }\n      if (forIfFlag) {\n        text = text.slice(0, -2);\n        this.rain = text;\n      } else {\n        text = \"You don't need to pack an umbrella in the next 5 days\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["Line","name","props","components","data","rain","title","computed","chartData1","labels","timeArray","backgroundColor","datasets","rainArray","chartOptions1","maintainAspectRatio","layout","padding","top","bottom","responsive","scales","y","ticks","display","grid","drawTicks","drawBorder","plugins","legend","tooltip","callbacks","label","context","parsed","chartData2","borderWidth","chartOptions2","x","afterFit","ctx","width","elements","point","radius","watch","newData","rainFlagWithDate","rainFlag","text","forIfFlag","key","concat","slice"],"sources":["/Users/SonodaMoeto/Desktop/Internet Application/Assignment 1/Weather_Forecast_App/src/components/Chart3.vue"],"sourcesContent":["<template>\n    <p class=\"rain\">{{title}}</p>\n    <div class=\"chartBox\">\n        <div class=\"colSmall\">\n            <Line id=\"my-chart-id\" :options=\"chartOptions2\" :data=\"chartData2\" v-if=\"display\"/>\n        </div>\n        <div class=\"colLarge\">\n            <div class=\"box\">\n                <Line id=\"my-chart-id\" :options=\"chartOptions1\" :data=\"chartData1\" v-if=\"display\"/>\n            </div>\n        </div>\n    </div>\n\n    <p class=\"text\">{{ rain }}</p>\n</template>\n\n<script>\n    import { Line } from 'vue-chartjs'\n    import 'chart.js/auto'\n\n    export default {\n        // eslint-disable-next-line vue/multi-word-component-names\n        name: \"Chart3\",\n        props: [\"rainArray\", \"rainFlagWithDate\", \"timeArray\", \"display\"],\n        components: { Line },\n        data () {\n            return {\n                rain: \"\",\n                title: \"\"\n            }\n        },\n        computed: {\n                chartData1() {\n                    return {\n                        labels: [ this.timeArray ],                        \n                        backgroundColor: '#f87979',\n                        datasets: [ { data: this.rainArray } ]\n                    }\n                },\n                chartOptions1() {\n                    return {\n                        maintainAspectRatio: false,\n                        layout: {\n                            padding: {\n                                top: 9,\n                                bottom: 15\n                            }\n                        },\n                        responsive: true,\n                        scales: {\n                            y: {\n                                ticks: {\n                                    display: false\n                                },\n                                grid: {\n                                    drawTicks: false,\n                                    drawBorder: false\n                                }\n                            }\n                        },\n                        plugins: {\n                            legend: {\n                                display: false\n                            },\n                            tooltip: {\n                                callbacks: {\n                                    label: function(context) {\n                                        return context.parsed.y + \"mm\"\n                                    }\n                                }\n                            }\n                        }\n                    }\n                },\n                chartData2() {\n                    return {\n                        labels: [ this.timeArray ],\n                        backgroundColor: '#f87979',\n                        datasets: [ \n                            { \n                                data: this.rainArray,\n                                borderWidth: 0\n                            } \n                        ]\n                    }\n                },\n                chartOptions2() {\n                    return {\n                        maintainAspectRatio: false,\n                        layout: {\n                            padding: {\n                                bottom: 44\n                            }\n                        },\n                        responsive: true,\n                        scales: {\n                            x: {\n                                ticks: {\n                                    display: false\n                                },\n                                grid: {\n                                    drawTicks:false\n                                }\n                            },\n                            y: {\n                                afterFit: (ctx) => {\n                                    ctx.width = 35\n                                }\n                            }\n                        },\n                        plugins: {\n                            legend: {\n                                display: false\n                            }\n                        },\n                        elements: {\n                            point:{\n                                radius: 0\n                            }\n                        }\n                    }\n                },\n            },\n        watch: {\n            rainArray: function(newData) {\n                this.chartData1.datasets[0].data = newData\n                this.chartData2.datasets[0].data = newData\n                this.title = \"Rain Volume(mm/3h)\"\n            },\n            timeArray: function(newData) {\n                this.chartData1.labels = newData\n                this.chartData2.labels = newData\n            },\n            rainFlagWithDate: function(newData) {\n                const rainFlag = newData\n                let text = \"You should pack an umbrella on \"\n                let forIfFlag = false\n                for(let key in rainFlag) {\n                    if(rainFlag[key]) {\n                        text = text.concat(key + \", \")\n                        forIfFlag = true\n                    }\n                }\n\n                if(forIfFlag) {\n                    text = text.slice(0, -2)\n                    this.rain = text\n                }\n                else {\n                    text = \"You don't need to pack an umbrella in the next 5 days\"\n                }\n            }\n        },\n    }\n</script>\n\n<style scoped>\n    .chartBox {\n        width: 700px;\n        display: flex;\n    }\n    .colSmall {\n        width: 35px;\n    }\n    .colLarge {\n        max-width: 700px;\n        overflow-x: scroll;\n    }\n    .box {\n        width: 2000px;\n        height: 500px;\n    }\n    .rain {\n        font-size: 20px;\n        font-weight: bold;\n    }\n    .text {\n        font-size: 18px;\n    }\n</style>\n"],"mappings":"AAiBI,SAASA,IAAG,QAAS,aAAY;AACjC,OAAO,eAAc;AAErB,eAAe;EACX;EACAC,IAAI,EAAE,QAAQ;EACdC,KAAK,EAAE,CAAC,WAAW,EAAE,kBAAkB,EAAE,WAAW,EAAE,SAAS,CAAC;EAChEC,UAAU,EAAE;IAAEH;EAAK,CA
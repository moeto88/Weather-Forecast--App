ta) {\n                this.chartData1.datasets[0].data = newData;\n                this.chartData2.datasets[0].data = newData;\n            },\n            timeArray: function(newData) {\n                this.chartData1.labels = newData;\n                this.chartData2.labels = newData;\n            },\n            rainFlagWithDate: function(newData) {\n                const rainFlag = newData\n                let text = \"You should pack an umbrella on \"\n                for(let key in rainFlag) {\n                    if(rainFlag.key) {\n                        text = text.concat(key + \", \")\n                        console.log(text)\n                    }\n                }\n                text = text.slice(0, -2)\n                this.rain = text\n            }\n        },\n    }\n</script>\n\n<style scoped>\n    .chartBox {\n        width: 700px;\n        display: flex;\n    }\n    .colSmall {\n        width: 35px;\n\n    }\n    .colLarge {\n        max-width: 700px;\n        overflow-x: scroll;\n    }\n    .box {\n        width: 2000px;\n        height: 500px;\n    }\n</style>\n"],"mappings":"AAgBI,SAASA,IAAG,QAAS,aAAY;AACjC,OAAO,eAAc;AAErB,eAAe;EACX;EACAC,IAAI,EAAE,QAAQ;EACdC,KAAK,EAAE,CAAC,WAAW,EAAE,kBAAkB,EAAE,WAAW,EAAE,SAAS,CAAC;EAChEC,UAAU,EAAE;IAAEH;EAAK,CAAC;EACpBI,IAAGA,CAAA,EAAK;IACJ,OAAO;MACHC,IAAI,EAAE;IACV;EACJ,CAAC;EACDC,QAAQ,EAAE;IACFC,UAAUA,CAAA,EAAG;MACT,OAAO;QACHC,MAAM,EAAE,CAAE,IAAI,CAACC,SAAQ,CAAG;QAC1BC,eAAe,EAAE,SAAS;QAC1BC,QAAQ,EAAE,CAAE;UAAEP,IAAI,EAAE,IAAI,CAACQ;QAAU;MACvC;IACJ,CAAC;IACDC,aAAaA,CAAA,EAAG;MACZ,OAAO;QACHC,mBAAmB,EAAE,KAAK;QAC1BC,MAAM,EAAE;UACJC,OAAO,EAAE;YACLC,GAAG,EAAE,CAAC;YACNC,MAAM,EAAE;UACZ;QACJ,CAAC;QACDC,UAAU,EAAE,IAAI;QAChBC,MAAM,EAAE;UACJC,CAAC,EAAE;YACCC,KAAK,EAAE;cACHC,OAAO,EAAE;YACb,CAAC;YACDC,IAAI,EAAE;cACFC,SAAS,EAAE,KAAK;cAChBC,UAAU,EAAE;YAChB;UACJ;QACJ,CAAC;QACDC,OAAO,EAAE;UACLC,MAAM,EAAE;YACJL,OAAO,EAAE;UACb,CAAC;UACDM,OAAO,EAAE;YACLC,SAAS,EAAE;cACPC,KAAK,EAAE,SAAAA,CAASC,OAAO,EAAE;gBACrB,OAAOA,OAAO,
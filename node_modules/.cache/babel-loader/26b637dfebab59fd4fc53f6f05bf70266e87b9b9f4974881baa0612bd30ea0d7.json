  }\n        },\n        plugins: {\n          legend: {\n            display: false\n          }\n        },\n        elements: {\n          point: {\n            radius: 0\n          }\n        }\n      };\n    }\n  },\n  watch: {\n    rainArray: function (newData) {\n      this.chartData1.datasets[0].data = newData;\n      this.chartData2.datasets[0].data = newData;\n    },\n    timeArray: function (newData) {\n      this.chartData1.labels = newData;\n      this.chartData2.labels = newData;\n    },\n    rainFlagWithDate: function (newData) {\n      const rainFlag = newData;\n      let text = \"You should pack an umbrella on \";\n      for (let key in rainFlag) {\n        if (rainFlag.key) {\n          text = text.concat(key + \", \");\n          console.log(text);\n        }\n      }\n      text = text.slice(0, -2);\n      this.rain = text;\n    }\n  }\n};","map":{"version":3,"names":["Line","name","props","components","data","rain","computed","chartData1","labels","timeArray","backgroundColor","datasets","rainArray","chartOptions1","maintainAspectRatio","layout","padding","top","bottom","responsive","scales","y","ticks","display","grid","drawTicks","drawBorder","plugins","legend","tooltip","callbacks","label","context","parsed","chartData2","borderWidth","chartOptions2","x","afterFit","ctx","width","elements","point","radius","watch","newData","rainFlagWithDate","rainFlag","text","key","concat","console","log","slice"],"sources":["/Users/SonodaMoeto/Desktop/Internet Application/vue-express/vue-express-sample/src/components/Chart3.vue"],"sourcesContent":["<template>\n    <div class=\"chartBox\">\n        <div class=\"colSmall\">\n            <Line id=\"my-chart-id\" :options=\"chartOptions2\" :data=\"chartData2\" v-if=\"display\"/>\n        </div>\n        <div class=\"colLarge\">\n            <div class=\"box\">\n                <Line id=\"my-chart-id\" :options=\"chartOptions1\" :data=\"chartData1\" v-if=\"display\"/>\n            </div>\n        </div>\n    </div>\n\n    <h3>{{ rain }}</h3>\n</template>\n\n<script>\n    import { Line } from 'vue-chartjs'\n    import 'chart.js/auto'\n\n    export default {\n        // eslint-disable-next-line vue/multi-word-component-names\n        name: \"Chart3\",\n        props: [\"rainArray\", \"rainFlagWithDate\", \"timeArray\", \"display\"],\n        components: { Line },\n        data () {\n            return {\n                rain: \"\"\n            }\n        },\n        computed: {\n                chartData1() {\n                    return {\n                        labels: [ this.timeArray ],                        \n                        backgroundColor: '#f87979',\n                        datasets: [ { data: this.rainArray } ]\n                    }\n                },\n                chartOptions1() {\n                    return {\n                        maintainAspectRatio: false,\n                        layout: {\n                            padding: {\n                                top: 9,\n                                bottom: 15\n                            }\n                        },\n                        responsive: true,\n                        scales: {\n                            y: {\n                                ticks: {\n                                    display: false\n                                },\n                                grid: {\n                                    drawTicks: false,\n                                    drawBorder: false\n                                }\n                            }\n                        },\n                        plugins: {\n                            legend: {\n                                display: false\n                            },\n                            tooltip: {\n                                callbacks: {\n                                    label: function(context) {\n                                        return context.parsed.y + \"mm\"\n                      
{"ast":null,"code":"import { Line } from 'vue-chartjs';\nimport 'chart.js/auto';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Chart2\",\n  props: [\"windArray\", \"timeArray\", \"display\"],\n  components: {\n    Line\n  },\n  computed: {\n    chartData1() {\n      return {\n        labels: [this.timeArray],\n        backgroundColor: '#f87979',\n        datasets: [{\n          data: this.windArray\n        }]\n      };\n    },\n    chartOptions1() {\n      return {\n        maintainAspectRatio: false,\n        layout: {\n          padding: {\n            top: 9,\n            bottom: 15\n          }\n        },\n        responsive: true,\n        scales: {\n          y: {\n            ticks: {\n              display: false\n            },\n            grid: {\n              drawTicks: false,\n              drawBorder: false\n            }\n          }\n        },\n        plugins: {\n          legend: {\n            display: false\n          },\n          tooltip: {\n            callbacks: {\n              label: function (context) {\n                const temp = parseInt(context.parsed.y);\n                if (temp < 13) {\n                  return context.parsed.y + \"°C Cold\";\n                } else if (temp >= 13 && temp <= 23) {\n                  return context.parsed.y + \"°C Mild\";\n                } else {\n                  return context.parsed.y + \"°C Hot\";\n                }\n              }\n            }\n          }\n        }\n      };\n    },\n    chartData2() {\n      return {\n        labels: [this.timeArray],\n        backgroundColor: '#f87979',\n        datasets: [{\n          data: this.windArray,\n          borderWidth: 0\n        }]\n      };\n    },\n    chartOptions2() {\n      return {\n        maintainAspectRatio: false,\n        layout: {\n          padding: {\n            bottom: 44\n          }\n        },\n        responsive: true,\n        scales: {\n          x: {\n            ticks: {\n              display: false\n            },\n            grid: {\n              drawTicks: false\n            }\n          },\n          y: {\n            afterFit: ctx => {\n              ctx.width = 35;\n            }\n          }\n        },\n        plugins: {\n          legend: {\n            display: false\n          }\n        },\n        elements: {\n          point: {\n            radius: 0\n          }\n        }\n      };\n    }\n  },\n  watch: {\n    windArray: function (newData) {\n      this.chartData1.datasets[0].data = newData;\n      this.chartData2.datasets[0].data = newData;\n    },\n    timeArray: function (newData) {\n      this.chartData1.labels = newData;\n      this.chartData2.labels = newData;\n    }\n  }\n};","map":{"version":3,"names":["Line","name","props","components","computed","chartData1","labels","timeArray","backgroundColor","datasets","data","windArray","chartOptions1","maintainAspectRatio","layout","padding","top","bottom","responsive","scales","y","ticks","display","grid","drawTicks","drawBorder","plugins","legend","tooltip","callbacks","label","context","temp","parseInt","parsed","chartData2","borderWidth","chartOptions2","x","afterFit","ctx","width","elements","point","radius","watch","newData"],"sources":["/Users/SonodaMoeto/Desktop/Internet Application/vue-express/vue-express-sample/src/components/Chart2.vue"],"sourcesContent":["<template>\n    <div class=\"chartBox\">\n        <div class=\"colSmall\">\n            <Line id=\"my-chart-id\" :options=\"chartOptions2\" :data=\"chartData2\" v-if=\"display\"/>\n        </div>\n        <div class=\"colLarge\">\n            <div class=\"box\">\n                <Line id=\"my-chart-id\" :options=\"chartOptions1\" :data=\"chartData1\" v-if=\"display\"/>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import { Line } from 'vue-chartjs'\n    import 'chart.js/auto'\n\n    export default {\n        // eslint-disable-next-line vue/multi-word-component-names\n        name: \"Chart2\",\n        props: [\"windArray\", \"timeArray\", \"display\"],\n        components: { Line },\n        computed: {\n                chartData1() {\n                    return {\n                        labels: [ this.timeArray ],                        \n                        backgroundColor: '#f87979',\n                        datasets: [ { data: this.windArray } ]\n                    }\n                },\n                chartOptions1() {\n                    return {\n                        maintainAspectRatio: false,\n                        layout: {\n                            padding: {\n                                top: 9,\n                                bottom: 15\n                            }\n                        },\n                        responsive: true,\n                        scales: {\n                            y: {\n                                ticks: {\n                                    display: false\n                                },\n                                grid: {\n                                    drawTicks: false,\n                                    drawBorder: false\n                                }\n                            }\n                        },\n                        plugins: {\n                            legend: {\n                                display: false\n                            },\n                            tooltip: {\n                                callbacks: {\n                                    label: function(context) {\n                                        const temp = parseInt(context.parsed.y)\n                                        if(temp < 13) {\n                                            return context.parsed.y + \"°C Cold\"\n                                        }\n                                        else if(temp >= 13 && temp <= 23) {\n                                            return context.parsed.y + \"°C Mild\"\n                                        }\n                                        else {\n                                            return context.parsed.y + \"°C Hot\"\n                                        }\n                                    }\n                                }\n                            }\n                        }\n                    }\n                },\n                chartData2() {\n                    return {\n                        labels: [ this.timeArray ],\n                        backgroundColor: '#f87979',\n                        datasets: [ \n                            { \n                                data: this.windArray,\n                                borderWidth: 0\n                            } \n                        ]\n                    }\n                },\n                chartOptions2() {\n                    return {\n                        maintainAspectRatio: false,\n                        layout: {\n                            padding: {\n                                bottom: 44\n                            }\n                        },\n                        responsive: true,\n                        scales: {\n                            x: {\n                                ticks: {\n                                    display: false\n                                },\n                                grid: {\n                                    drawTicks:false\n                                }\n                            },\n                            y: {\n                                afterFit: (ctx) => {\n                                    ctx.width = 35\n                                }\n                            }\n                        },\n                        plugins: {\n                            legend: {\n                                display: false\n                            }\n                        },\n                        elements: {\n                            point:{\n                                radius: 0\n                            }\n                        }\n                    }\n                }\n            },\n        watch: {\n            windArray: function(newData) {\n                this.chartData1.datasets[0].data = newData;\n                this.chartData2.datasets[0].data = newData;\n            },\n            timeArray: function(newData) {\n                this.chartData1.labels = newData;\n                this.chartData2.labels = newData;\n            },\n        },\n    }\n</script>\n\n<style scoped>\n    .chartBox {\n        width: 700px;\n        display: flex;\n    }\n    .colSmall {\n        width: 35px;\n\n    }\n    .colLarge {\n        max-width: 700px;\n        overflow-x: scroll;\n    }\n    .box {\n        width: 2000px;\n        height: 500px;\n    }\n</style>\n"],"mappings":"AAcI,SAASA,IAAG,QAAS,aAAY;AACjC,OAAO,eAAc;AAErB,eAAe;EACX;EACAC,IAAI,EAAE,QAAQ;EACdC,KAAK,EAAE,CAAC,WAAW,EAAE,WAAW,EAAE,SAAS,CAAC;EAC5CC,UAAU,EAAE;IAAEH;EAAK,CAAC;EACpBI,QAAQ,EAAE;IACFC,UAAUA,CAAA,EAAG;MACT,OAAO;QACHC,MAAM,EAAE,CAAE,IAAI,CAACC,SAAQ,CAAG;QAC1BC,eAAe,EAAE,SAAS;QAC1BC,QAAQ,EAAE,CAAE;UAAEC,IAAI,EAAE,IAAI,CAACC;QAAU;MACvC;IACJ,CAAC;IACDC,aAAaA,CAAA,EAAG;MACZ,OAAO;QACHC,mBAAmB,EAAE,KAAK;QAC1BC,MAAM,EAAE;UACJC,OAAO,EAAE;YACLC,GAAG,EAAE,CAAC;YACNC,MAAM,EAAE;UACZ;QACJ,CAAC;QACDC,UAAU,EAAE,IAAI;QAChBC,MAAM,EAAE;UACJC,CAAC,EAAE;YACCC,KAAK,EAAE;cACHC,OAAO,EAAE;YACb,CAAC;YACDC,IAAI,EAAE;cACFC,SAAS,EAAE,KAAK;cAChBC,UAAU,EAAE;YAChB;UACJ;QACJ,CAAC;QACDC,OAAO,EAAE;UACLC,MAAM,EAAE;YACJL,OAAO,EAAE;UACb,CAAC;UACDM,OAAO,EAAE;YACLC,SAAS,EAAE;cACPC,KAAK,EAAE,SAAAA,CAASC,OAAO,EAAE;gBACrB,MAAMC,IAAG,GAAIC,QAAQ,CAACF,OAAO,CAACG,MAAM,CAACd,CAAC;gBACtC,IAAGY,IAAG,GAAI,EAAE,EAAE;kBACV,OAAOD,OAAO,CAACG,MAAM,CAACd,CAAA,GAAI,SAAQ;gBACtC,OACK,IAAGY,IAAG,IAAK,EAAC,IAAKA,IAAG,IAAK,EAAE,EAAE;kBAC9B,OAAOD,OAAO,CAACG,MAAM,CAACd,CAAA,GAAI,SAAQ;gBACtC,OACK;kBACD,OAAOW,OAAO,CAACG,MAAM,CAACd,CAAA,GAAI,QAAO;gBACrC;cACJ;YACJ;UACJ;